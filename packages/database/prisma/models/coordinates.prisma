model Coordinates {
  id        String   @id @default(cuid())
  x         Int
  y         Int
  width     Int
  height    Int

  location  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  factionId String?
  faction   Faction? @relation(fields: [factionId], references: [id], onDelete: Cascade)
}
